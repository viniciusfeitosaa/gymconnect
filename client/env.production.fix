# Configurações para resolver problemas de build no Netlify
# Copie este arquivo para .env.production

NODE_ENV=production
REACT_APP_API_URL=/.netlify/functions/api
REACT_APP_NETLIFY_URL=https://your-app.netlify.app

# Desabilitar warnings como erros
GENERATE_SOURCEMAP=false
CI=false
DISABLE_ESLINT_PLUGIN=true

# Configurações adicionais para build
SKIP_PREFLIGHT_CHECK=true
FAST_REFRESH=false
